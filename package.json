{
  "name": "gitproject",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:qa": "TEST_ENV=qa npx playwright test",
    "test:uat": "TEST_ENV=uat npx playwright test",
    "e2e:test": "playwright test -c e2e/playwright.config.js",
    "e2e:test:headed": "playwright test -c e2e/playwright.config.js --headed",
    "e2e:test:ui": "playwright test -c e2e/playwright.config.js --ui",
    "e2e:report": "playwright show-report e2e/reports/html",
    "e2e:trace": "playwright show-trace ./e2e/test-results/**/trace.zip",
    "e2e:test:filing": "playwright test -c e2e/playwright.config.js -g @filing",
    "e2e:test:hearings": "playwright test -c e2e/playwright.config.js -g @hearings",
    "e2e:test:join": "playwright test -c e2e/playwright.config.js -g @join",
    "e2e:test:pending": "playwright test -c e2e/playwright.config.js -g @pending"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^22.15.19"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pucardotorg/Qa-Automation.git"
  },
  "bugs": {
    "url": "https://github.com/pucardotorg/Qa-Automation/issues"
  },
  "homepage": "https://github.com/pucardotorg/Qa-Automation#readme",
  "description": ""
}
